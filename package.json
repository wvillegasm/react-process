  "repository": "https://github.com/wvillegasm/react-process.git",
  "main": "index.js",
  "scripts": {
    "test": "env NODE_PATH=$NODE_PATH:$PWD/src mocha --require @babel/register --require ignore-styles --require test/helper/setup.js 'src/**/*-spec.@(js|jsx)' --reporter spec --use_strict --throw-deprecation --watch",
    "lint": "eslint --color --ext js --ext jsx ./src | snazzy",
    "standard": "standard --verbose --parser babel-eslint | snazzy",
    "build": "rimraf dist && webpack --env.NODE_ENV=production -p",
    "dev": "webpack-dev-server --env.NODE_ENV=local",
    "dll": "rimraf vendor && webpack  --config webpack.dll.config.js -p",
    "postinstall": "npm run dll",
    "clean": "rimraf dist",
    "clean:dll": "rimraf dist",
    "lint:std": "standard --verbose --parser babel-eslint --env mocha --env es6 --env node | snazzy;exit 0"
  },
